---
import type { CollectionEntry } from 'astro:content'

interface Props {
    /** Size of the spacer, e.g. "1rem". */
    size: string
}

const { size } = Astro.props
---

<div
    class="spacer"
    style={{
        width: size,
        height: size,
    }}
>
</div>

<style>
    .spacer {
    }
</style>
